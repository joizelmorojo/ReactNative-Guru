{
  "expo": {
    "name": "RetoGuru",
    "slug": "RetoGuru",
    "version": "1.3.6",
    "orientation": "portrait",
    "icon": "./assets/icono.png",
    "splash": {
      "image": "./assets/splash3.png",
      "resizeMode": "cover",
      "backgroundColor": "#2196f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icono_apple.png",
      "bundleIdentifier": "net.tecgurus.retoguru",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "RetoGuru needs to access your camera roll in order to change your profile photo in your account",
        "NSPhotoLibraryAddUsageDescription": "RetoGuru needs to access your camera roll in order to change your profile photo in your account"
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1.3.6"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/icono.png",
      "color": "#311b92",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Colab.notes"
    },
    "android": {
      "package": "net.tecgurus.retoguru",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/icono.png",
      "useNextNotificationsApi": true,
      "versionCode": 36,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "CAMERA",
        "CAMERA_ROLL",
        "READ_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "description": ""
  }
}
